<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title></title>
    </head>
    <body>
<h3><a href = "/users/<%=@user.id%>">Dashboard</a></h3>

        <h3><%=@user.first_name%> <%=@user.last_name%>'s Playlist:</h3>
<table>

<th>Artist</th>
<th>Title</th>
<th>Times Added</th>


<% @list.uniq.each do |x|%>
<tr>
<td><%=x.artist%></td>
<td><%=x.title%></td>
<td><%=@added.where(song_id:x.id).count%></td>
</tr>
<%end%>

</table>

    </body>
</html>
